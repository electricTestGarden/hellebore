---
import { getCollection } from 'astro:content';
import PageLayout from "../layouts/pageLayout.astro";
import Character from "../components/character.astro";
import { getSortedCharacters } from '../utils';

const characters = await getSortedCharacters();
---

<PageLayout titleOverride="Characters">
    {characters.map((character) => <Character character={character}/>)}
</PageLayout>
